	вот а -24	; 0xffffffe8
	вот б -28	; 0xffffffe4
	вот в -32	; 0xffffffe0
	вот г -36	; 0xffffffdc
	вот чето_переменная -37	; 0xffffffdb
	вот функция -45	; 0xffffffd3
	вот не -49	; 0xffffffcf
	вот ско -49	; 0xffffffcf
	вот ль -49	; 0xffffffcf
	вот ко -49	; 0xffffffcf
