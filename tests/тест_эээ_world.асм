участок чит исп
глав_фц{ч32_[фц{ч32_ч32_лик_Йцук_ч32}_2]_тлен}:
	толк рбп
	быть рбп рсп
	вот аргов -4	; 0xfffffffc
	быть (рсп аргов) е8
	вот арги -12	; 0xfffffff4
	быть (рсп арги) р9
	минс рсп 12
	быть рсп рбп
	выт рбп
	возд

