	толк рбп
	быть рбп рсп
	вот аргов -4	; 0xfffffffc
	быть (рбп аргов) е8
	вот арги -12	; 0xfffffff4
	быть (рбп арги) р9
;; 0х00 ######################## ЗДЕСЬ НАЧАЛО ФУНКЦИИ ######################## ;;
	вот а -16	; 0xfffffff0
	вот б -20	; 0xffffffec
	вот в -24	; 0xffffffe8
	вот г -28	; 0xffffffe4
	вот чето_переменная -29	; 0xffffffe3
	вот функция -37	; 0xffffffdb
	вот не -41	; 0xffffffd7
	вот ско -41	; 0xffffffd7
	вот ль -41	; 0xffffffd7
	вот ко -41	; 0xffffffd7
;; ######################## ЗДЕСЬ ЦЕНТР  ФУНКЦИИ ######################## ;;
	идти глав_{ч32_[{ч32_ч32_лик_Йцук_счет_ЧЕТО_ч32}_2]_тлен}пропустить_первый_вывод

