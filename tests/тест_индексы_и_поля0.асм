просто_индекс__{тлен}:
	толк рбп
	быть рбп рсп
	; (гчисла[3] = 42)
	быть чбайт (гчисла__[ч32_100] + 12) 42	; 0x0000002a
	; (гг-@гандоны[2]-@рабов = 5)
	быть чбайт (гг__лик_Галера + Галера.гандоны + 664 + Начальник.рабов) 5	; 0x00000005
	; (гг_ук->гандоны[1]-@рабы[3]-@возр = 30)
	быть рах вбайт (гг_ук__ук_лик_Галера)
	быть чбайт (рах Галера.гандоны + 332 + Начальник.рабы + 96 + Раб.возр) 30	; 0x0000001e
	выт рбп
	возд

тест_переменная_как_индекс__{тлен}:
	толк рбп
	быть рбп рсп
	вот ь -4	; 0xfffffffc
	вот ъ -8	; 0xfffffff8
	; (ь = 2)
	быть чбайт (рбп ь) 2	; 0x00000002
	; (ъ = 3)
	быть чбайт (рбп ъ) 3	; 0x00000003
	; (гчисла[ь] = 100)
	быть рах чбайт (рбп ь)
	быть чбайт (4 рах гчисла__[ч32_100]) 100	; 0x00000064
	; (гг-@гандоны[ь]-@рабы[ъ]-@копейки = 50000.0)
	быть рах чбайт (рбп ъ)
	сдал рах 5	; 0x00000005
	быть рсх чбайт (рбп ь)
	зумн рсх рсх 332	; 0x0000014c
	плюс рсх рах
	быть рах 50000.
