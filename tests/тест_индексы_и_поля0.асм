	; (гг_ук->гандоны[0]-@рабы[ь]-@возр = 25)
	быть рах чбайт (рбп ь)
	сдал рах 5	; 0x00000005
	быть рсх вбайт (гг_ук__ук_лик_Галера)
	плюс рсх рах
	быть чбайт (рсх Галера.гандоны + Начальник.рабы + Раб.возр) 25	; 0x00000019
	выт рбп
	возд

