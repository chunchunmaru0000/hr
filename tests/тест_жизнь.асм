	сзов
	выт рбп
	возд

прервать__{тлен}:
	толк рбп
	быть рбп рсп

	искл рах рах
	быть вбайт (рбп -16) рах

	быть рах вбайт 50_000_000
	быть вбайт (рбп -8) рах

	быть еах 35
	задр рди (рбп -16)
	искл рси рси
	сзов
	выт рбп
	возд

вывести_поле__{тлен}:
	толк рбп
	быть рбп рсп
	минс рсп 8	; 0x00000008
	вот х -4	; 0xfffffffc
	вот у -8	; 0xfffffff8
	; вещать(очистка_консоли_и_перевод_в_начало)
	быть рах вбайт (очистка_консоли_и_перевод_в_начало__ук_ц8)
	быть рси рах
	зов вещать__{ук_ц8_тлен}
	; (х = 0)
	быть чбайт (рбп х) 0	; 0x00000000
	_в0:
		; (у = 0)
		быть чбайт (рбп у) 0	; 0x00000000
		_в1:
			; вещать({поле[х][у] ? жизнь : смерть})
			быть рах чбайт (рбп у)
			быть рсх чбайт (рбп х)
			сдал рсх 5	; 0x00000005
			плюс рсх рах
			быть рсх байт (рсх поле__[[ч8_32]_32])
			проб рсх рсх
			ид0 _и0
			быть рах вбайт (жизнь__ук_ц8)
			идти _и1
			_и0:
			быть рах вбайт (смерть__ук_ц8)
			_и1:
			быть рси рах
			зов вещать__{ук_ц8_тлен}
			; ?? ((++у) >= 32)
			быть еах чбайт (рбп у)
			плюс еах 1	; 0x00000001
			быть чбайт (рбп у) еах
			срав еах 32	; 0x00000020
			идм _и2
				идти вывести_поле__{тлен}после_у
			_и2:
		идти _в1
	вывести_поле__{тлен}после_у:
		; вещать(Эээ)
		быть рах вбайт (Эээ__ук_ц8)
		быть рси рах
		зов вещать__{ук_ц8_тлен}
		; ?? ((++х) >= 32)
		быть еах чбайт (рбп х)
		плюс еах 1	; 0x00000001
		быть чбайт (рбп х) еах
		срав еах 32	; 0x00000020
