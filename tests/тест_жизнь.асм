участок чит исп

вещать__{ук_ц8_тлен}:
	толк рбп
	быть рбп рсп
	вот строка -8	; 0xfffffff8
	быть (рбп строка) рси

	быть рди рси
	искл еах еах
	искл рсх рсх
	не рсх
	орф
	повтн0 осмсб
	быть рдх рди

	++еах 				; еах = write(1)
	быть еди еах	 	; еди = stdout(1)
						; рси строка
	минс рдх рси --едх	; длина строки
	сзов
	выт рбп
	возд

прервать__{тлен}:
	толк рбп
	быть рбп рсп

	искл рах рах
	быть вбайт (рбп -16) рах

	быть рах вбайт 50_000_000
	быть вбайт (рбп -8) рах

	быть еах 35
	задр рди (рбп -16)
	искл рси рси
	сзов
	выт рбп
	возд

вывести_поле__{тлен}:
	толк рбп
	быть рбп рсп
	минс рсп 8	; 0x00000008
	вот х -4	; 0xfffffffc
	вот у -8	; 0xfffffff8
	; вещать(очистка_консоли_и_перевод_в_начало)
	быть рах вбайт (очистка_консоли_и_перевод_в_начало__ук_ц8)
	быть рси рах
	зов вещать__{ук_ц8_тлен}
	; х => (0 ... 16) ( ... )
	; (х => 0)
	быть чбайт (рбп х) 0	; 0x00000000
	идти _и2
	_и1:
		; у => (0 ... 16) ( ... )
		; (у => 0)
		быть чбайт (рбп у) 0	; 0x00000000
		идти _и4
		_и3:
			; вещать({поле[х][у] ? жизнь : смерть})
			быть рах чбайт (рбп у)
			быть рсх чбайт (рбп х)
			сдал рсх 4	; 0x00000004
			плюс рсх рах
			быть рсх байт (рсх поле__[[ч8_16]_16])
			проб рсх рсх
			ид0 _и5
			быть рах вбайт (жизнь__ук_ц8)
			идти _и6
			_и5:
			быть рах вбайт (смерть__ук_ц8)
			_и6:
			быть рси рах
			зов вещать__{ук_ц8_тлен}
		плюс чбайт (рбп у) 1	; 0x00000001
		_и4:
		срав чбайт (рбп у) 16	; 0x00000010
		идм _и3
		; вещать(Эээ)
		быть рах вбайт (Эээ__ук_ц8)
		быть рси рах
		зов вещать__{ук_ц8_тлен}
	плюс чбайт (рбп х) 1	; 0x00000001
	_и2:
	срав чбайт (рбп х) 16	; 0x00000010
	идм _и1
	выйти
	возд

живых_рядом__{ч32_ч32_ч32}:
	толк рбп
	быть рбп рсп
	вот х -4	; 0xfffffffc
	быть (рбп х) еси
	вот у -8	; 0xfffffff8
	быть (рбп у) еди
	вот ь -12	; 0xfffffff4
	вот ъ -16	; 0xfffffff0
	вот хь -20	; 0xffffffec
	вот уъ -24	; 0xffffffe8
	вот живых -28	; 0xffffffe4
	; (живых = 0)
	быть чбайт (рбп живых) 0	; 0x00000000
	; ь => (-1 ..= 1) ( ... )
	; (ь => -1)
	быть чбайт (рбп ь) -1	; 0xffffffff
	идти _и8
	_и7:
		; ъ => (-1 ..= 1) ( ... )
		; (ъ => -1)
		быть чбайт (рбп ъ) -1	; 0xffffffff
		идти _и10
		_и9:
			; ?? ((!ь) && (!ъ))
			быть еах чбайт (рбп ь)
			проб еах еах
			идн0 _и11
			быть еах чбайт (рбп ъ)
			проб еах еах
			идн0 _и11
				идти _и0
			_и11:
			; (хь = (х + ь))
			быть еах чбайт (рбп ь)
			плюс еах чбайт (рбп х)
			быть чбайт (рбп хь) еах
			; (уъ = (у + ъ))
			быть еах чбайт (рбп ъ)
			плюс еах чбайт (рбп у)
			быть чбайт (рбп уъ) еах
			; (живых += (живых += поле[{(хь < 0) ? 15 : {(хь < 16) ? хь : 0}}][{(уъ < 0) ? 15 : {(уъ < 16) ? уъ : 0}}]))
			срав чбайт (рбп уъ) 0	; 0x00000000
			идбр _и12
			быть еах 15	; 0x0000000f
			идти _и13
			_и12:
			срав чбайт (рбп уъ) 16	; 0x00000010
			идбр _и14
			быть еах чбайт (рбп уъ)
			идти _и15
			_и14:
			искл еах еах
			_и15:
			_и13:
			перечв
			срав чбайт (рбп хь) 0	; 0x00000000
			идбр _и16
			быть есх 15	; 0x0000000f
			идти _и17
			_и16:
			срав чбайт (рбп хь) 16	; 0x00000010
			идбр _и18
			быть есх чбайт (рбп хь)
			идти _и19
			_и18:
			искл есх есх
			_и19:
			_и17:
			быть рсх есх
			сдал рсх 4	; 0x00000004
			плюс рсх рах
			быть рсх байт (рсх поле__[[ч8_16]_16])
			быть еах чбайт (рбп живых)
			перечв
			плюс рсх рах
			быть чбайт (рбп живых) есх
		_и0:
		плюс чбайт (рбп ъ) 1	; 0x00000001
		_и10:
		срав чбайт (рбп ъ) 1	; 0x00000001
		идмр _и9
	плюс чбайт (рбп ь) 1	; 0x00000001
	_и8:
	срав чбайт (рбп ь) 1	; 0x00000001
	идмр _и7
	быть еах чбайт(рбп живых)
	выт рбп
	возд

пережить__{тлен}:
	толк рбп
	быть рбп рсп
	минс рсп 12	; 0x0000000c
	вот х -4	; 0xfffffffc
	вот у -8	; 0xfffffff8
	вот живых -12	; 0xfffffff4
	; х => (0 ... 16) ( ... )
	; (х => 0)
	быть чбайт (рбп х) 0	; 0x00000000
	идти _и21
	_и20:
		; у => (0 ... 16) ( ... )
		; (у => 0)
		быть чбайт (рбп у) 0	; 0x00000000
		идти _и23
		_и22:
			; (живых = живых_рядом(х, у))
			быть еах чбайт (рбп х)
			быть есх чбайт (рбп у)
			быть рси рах
			быть рди рсх
			зов живых_рядом__{ч32_ч32_ч32}
			быть чбайт (рбп живых) еах
			; (эволюция[х][у] = {поле[х][у] ? ((живых == 2) || (живых == 3)) : (живых == 3)})
			быть рах чбайт (рбп у)
			быть рсх чбайт (рбп х)
			сдал рсх 4	; 0x00000004
			плюс рсх рах
			быть рах чбайт (рбп у)
			быть рдх чбайт (рбп х)
			сдал рдх 4	; 0x00000004
			плюс рдх рах
			быть рдх байт (рдх поле__[[ч8_16]_16])
			проб рдх рдх
			ид0 _и24
			срав чбайт (рбп живых) 2	; 0x00000002
			идр _и26
			срав чбайт (рбп живых) 3	; 0x00000003
			иднр _и27
			_и26:
			быть еах 1	; 0x00000001
			идти _и28
			_и27:
			искл еах еах
			_и28:
			идти _и25
			_и24:
			срав чбайт (рбп живых) 3	; 0x00000003
			устр ал
			_и25:
			быть еах ал
			быть байт (рсх эволюция__[[ч8_16]_16]) ал
		плюс чбайт (рбп у) 1	; 0x00000001
		_и23:
		срав чбайт (рбп у) 16	; 0x00000010
		идм _и22
	плюс чбайт (рбп х) 1	; 0x00000001
	_и21:
	срав чбайт (рбп х) 16	; 0x00000010
	идм _и20
	; х => (0 ... 16) ( ... )
	; (х => 0)
	быть чбайт (рбп х) 0	; 0x00000000
	идти _и30
	_и29:
		; у => (15 ..= 0 назад) ( ... )
		; (у => 15)
		быть чбайт (рбп у) 15	; 0x0000000f
		идти _и32
		_и31:
			; (поле[х][у] = эволюция[х][у])
			быть рах чбайт (рбп у)
			быть рсх чбайт (рбп х)
			сдал рсх 4	; 0x00000004
			плюс рсх рах
			быть рах чбайт (рбп у)
			быть рдх чбайт (рбп х)
			сдал рдх 4	; 0x00000004
			плюс рдх рах
			быть рдх байт (рдх эволюция__[[ч8_16]_16])
			быть байт (рсх поле__[[ч8_16]_16]) дл
		минс чбайт (рбп у) 1	; 0x00000001
		_и32:
		срав чбайт (рбп у) 0	; 0x00000000
		идбр _и31
	плюс чбайт (рбп х) 1	; 0x00000001
	_и30:
	срав чбайт (рбп х) 16	; 0x00000010
	идм _и29
	выйти
	возд

вход глав глав:глав__{ч32_ук_ук_ц8_тлен}:
	толк рбп
	быть рбп рсп
	минс рсп 12	; 0x0000000c
	вот аргов -4	; 0xfffffffc
	быть (рбп аргов) еси
	вот арги -12	; 0xfffffff4
	быть (рбп арги) рди
	_в0:
		; вывести_поле()
		зов вывести_поле__{тлен}
		; пережить()
		зов пережить__{тлен}
		; прервать()
		зов прервать__{тлен}
	идти _в0
	выйти
	возд

