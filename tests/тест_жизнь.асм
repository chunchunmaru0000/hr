_у0:
	пусть байт "\x1B[H" 0
_у1:
	пусть байт "Эээ world\n" 0
_у2:
	пусть байт "██" 0
_у3:
	пусть байт "__" 0

участок чит исп

вещать__{ук_ц8_тлен}:
	толк рбп
	быть рбп рсп
	вот строка -8	; 0xfffffff8
	быть (рбп строка) рси

	быть рди рси
	искл еах еах
	искл рсх рсх
	не рсх
	орф
	повтн0 осмсб
	быть рдх рди

	++еах 				; еах = write(1)
	быть еди еах	 	; еди = stdout(1)
						; рси строка
