	возд

живых_рядом__{ч32_ч32_ч32}:
	толк рбп
	быть рбп рсп
	вот х -4	; 0xfffffffc
	быть (рбп х) еси
	вот у -8	; 0xfffffff8
	быть (рбп у) еди
	вот ь -12	; 0xfffffff4
	вот ъ -16	; 0xfffffff0
	вот хь -20	; 0xffffffec
	вот уъ -24	; 0xffffffe8
	вот живых -28	; 0xffffffe4
	; (живых = 0)
	быть чбайт (рбп живых) 0	; 0x00000000
	; (ь = -1)
	быть чбайт (рбп ь) -1	; 0xffffffff
	_в2:
		; (ъ = -1)
		быть чбайт (рбп ъ) -1	; 0xffffffff
		_в3:
			; ?? ((!ь) && (!ъ))
			быть еах чбайт (рбп ь)
			проб еах еах
			идн0 _и4
			быть еах чбайт (рбп ъ)
			проб еах еах
			идн0 _и4
				идти живых_рядом__{ч32_ч32_ч32}пропустить_клетку
			_и4:
			; (хь = (х + ь))
			быть еах чбайт (рбп ь)
			плюс еах чбайт (рбп х)
			быть чбайт (рбп хь) еах
			; (уъ = (у + ъ))
			быть еах чбайт (рбп ъ)
			плюс еах чбайт (рбп у)
			быть чбайт (рбп уъ) еах
			; (живых += (живых += поле[{(хь < 0) ? 31 : {(хь < 32) ? хь : 0}}][{(уъ < 0) ? 31 : {(уъ < 32) ? уъ : 0}}]))
			срав чбайт (рбп уъ) 0	; 0x00000000
			идбр _и5
			быть еах 31	; 0x0000001f
			идти _и6
			_и5:
			срав чбайт (рбп уъ) 32	; 0x00000020
			идбр _и7
			быть еах чбайт (рбп уъ)
			идти _и8
			_и7:
			искл еах еах
			_и8:
			_и6:
			перечв
			срав чбайт (рбп хь) 0	; 0x00000000
			идбр _и9
			быть есх 31	; 0x0000001f
			идти _и10
			_и9:
			срав чбайт (рбп хь) 32	; 0x00000020
			идбр _и11
			быть есх чбайт (рбп хь)
			идти _и12
			_и11:
			искл есх есх
			_и12:
			_и10:
			быть рсх есх
			сдал рсх 5	; 0x00000005
			плюс рсх рах
			быть рсх байт (рсх поле__[[ч8_32]_32])
			быть еах чбайт (рбп живых)
			перечв
			плюс рсх рах
			быть чбайт (рбп живых) есх
		живых_рядом__{ч32_ч32_ч32}пропустить_клетку:
			; ?? ((++ъ) >= 2)
			быть еах чбайт (рбп ъ)
			плюс еах 1	; 0x00000001
			быть чбайт (рбп ъ) еах
			срав еах 2	; 0x00000002
			идм _и13
				идти живых_рядом__{ч32_ч32_ч32}после_ъ
			_и13:
		идти _в3
	живых_рядом__{ч32_ч32_ч32}после_ъ:
		; ?? ((++ь) >= 2)
		быть еах чбайт (рбп ь)
		плюс еах 1	; 0x00000001
		быть чбайт (рбп ь) еах
		срав еах 2	; 0x00000002
		идм _и14
			идти живых_рядом__{ч32_ч32_ч32}после_ь
