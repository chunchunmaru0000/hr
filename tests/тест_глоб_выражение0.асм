вот Чел.ф	0	; 0x00000000
вот Чел.и	8	; 0x00000008
вот Чел.о	16	; 0x00000010
вот Чел.возраст	24	; 0x00000018

участок чит изм

сложение_чисел__в64:
	пусть вбайт 6.2400000000000002131628207280300557613372802734375
сложение_строк__ук_ц8:
	пусть вбайт _у0
сложение_строк_и_чисел__ук_ц8:
	пусть вбайт _у1
умножение_строк__ук_ц8:
	пусть вбайт _у2
там_типа_смещения__ч32:
	пусть чбайт 512	; 0x00000200
тест_от_дипсика__ч64:
	пусть вбайт 1	; 0x00000001
мера_типа__ч32:
	пусть чбайт 26	; 0x0000001a
_у0:
	пусть байт "10 сложение 10 строк 10" 0
_у1:
	пусть байт "было 10 негров стало 9.5 негров, куда улетело 0.5 негра" 0
_у2:
	пусть байт "10_э__э__э__э__э__э__э__э__э__э__э_10" 0

участок чит исп


вход здесь
здесь:
	быть еах 1
	быть еди 1
	быть еси текст
	быть едх 20
	сзов
	быть еах 60
	быть еди 77
	сзов
пусть текст байт "\т\т\т\т\т\тэээ world!" 10
