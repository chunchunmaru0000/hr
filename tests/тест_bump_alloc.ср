#влечь (
	"выход.ср"
	"../б/ввыв/консоль/всё.ср"
	"../б/память/пласт.ср"
)

пусть запятая: стр = ", ",
пусть кс: стр = "\n",
пусть пск: стр = "[",
пусть лск: стр = "]",

_асм "вход глав глав:"
фц глав(аргов: ч32 арги: *стр) тлен (
	вещать..(123, запятая, 123, кс),


	пусть ( ч1 ч2: *ч64 ч: ч64 )
	__РАЗМЕР_ПЛАСТА = 0x1000,
	начать_пласт(),

	*(ч1 = пластом(0x1000)) = 1111,
	*(ч2 = пластом(0x1000)) = 2222,
	вещать..(*ч1, пск, (ч = ч1), лск, кс),
	вещать..(*ч2, пск, (ч = ч2), лск, кс),
	*(ч1 = пластом(0x1001)) = 3333,
	вещать..(__РАЗМЕР_ПЛАСТА, кс),
	*(ч2 = пластом(0x1012)) = 4444,
	вещать..(__РАЗМЕР_ПЛАСТА, кс),
	вещать..(*ч1, пск, (ч = ч1), лск, кс),
	вещать..(*ч2, пск, (ч = ч2), лск, кс),


	вещать..(123, запятая, 123, кс),
	выйти с кодом 77 .
)

