лик Чел ( ф и о: стр возраст: ц16 )
пусть чел0: лик Чел = { "ф0", "и0", "о0", 0}
пусть чел1: лик Чел = { и = "и1", ф = "ф1", возраст = 1, о = "о1" }
пусть чел2: лик Чел = { ф = "ф2" }
;пусть чел3: лик Чел = { ф = "ф3", ф = "ы", ы = "ф" }
пусть чел4: лик Чел = { }
пусть мера_типа: ч32 = мера &лик Чел
пусть окак0: [[ч32]] = [
	окак [ч32] [окак ч32 4, 5, 6],
	[1, 2, 3],
	окак [ч32] окак ч32 7
]
пусть массив_не0: [ч32] = 101
пусть массив_не1: [ч32 1] = 101
пусть массив_не2: [ч32 2] = 101
пусть не_массив0: ч32 = [32, 123, [312, 123321], 1]
пусть не_массив1: ч32 = []
пусть массив0: [ч32] = [1, 2, 3]
пусть массив1: [ч32 4] = [1, 2, 3]
пусть сложение_чисел: в64 = 5 + 2.24 - 1
пусть сложение_строк: стр = 10 + " сложение " + 10 + " строк " + 10
пусть сложение_строк_и_чисел: стр =
	"было " + 10 + " негров стало " + 9.5  +
	" негров, куда " + "улетело " + 0.5 + " негра"
пусть умножение_строк: стр = 10 + "_э_" * 10 + 10
пусть там_типа_смещения: ч32 = 2 << 8
пусть тест_от_дипсика: ч64 =
!!!!(
	( (10 + 5) * (7 - 3) / 2 ) &	; Арифметика: +, -, *, /
	( (15 ^ 6) | (8 & 3) ) ^    	; Побитовые: ^, |, &
	~( (1 << 3) ) &&            	; Побитовые: ~, <<
	( (100 > 50) || (20 <= 10) )	; Логические: &&, ||, >, <=
)
пусть с_переменными: ч32 = сложение_чисел + тест_от_дипсика

