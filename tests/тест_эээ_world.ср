
лик Чел (
	имя фамилия отчество прочее/еще: стр
	возраст/взр: ц8

	чето_там: [*тлен 10], ктото_там: [лик Чел], третье: лик Негра
	первое второе тре3ье: ц64

	а: ч32, б: ц32, в/г: в32, ж: ч32
)

фц глав (аргов: ч32 арги: [(ч32 ч32 лик Йцук счет ЧЕТО ! ч32) 2]) тлен (
_асм ";; 0х00 ######################## ЗДЕСЬ НАЧАЛО ФУНКЦИИ ######################## ;;
"
	пусть (
		 а  б в г: ч32
;		_а _б в г: ч32
		чето_переменная: ц8
		функция: (ч32 ч32 ! ч32)
		не/ско/ль/ко: ч32
	)
_асм ";; ######################## ЗДЕСЬ ЦЕНТР  ФУНКЦИИ ######################## ;;
"

	идти пропустить_первый_вывод

_асм "
	быть еах 1
	быть еди 1
	быть еси текст
	быть едх 20
	сзов
"

	пропустить_первый_вывод:

_асм "
	быть еах 1
	быть еди 1
	быть еси текст
	быть едх 20
	сзов
"

_асм ";; ######################## ЗДЕСЬ КОНЕЦ  ФУНКЦИИ ######################## ;;
"
)

