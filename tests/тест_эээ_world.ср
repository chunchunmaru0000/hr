
лик Чел (
	имя фамилия отчество прочее/еще: стр
	возраст/взр: ц8

	чето_там: *тлен, ктото_там: [лик Чел], третье: лик Негра
	первое второе тре3ье: ц64

	а: ч32, б: ц32, в/г: в32, ж: ч32
)

фц глав (аргов: ч32 арги: *(ч32 ч32 лик Йцук счет ЧЕТО ! ч32)) тлен (
_асм ";; 0х00 ######################## ЗДЕСЬ НАЧАЛО ФУНКЦИИ ######################## ;;
"
	пусть (
		 а  б в г: ч32
;		_а _б в г: ч32
		чето_переменная: ц8
		функция: (ч32 ч32 ! ч32)
		не/ско/ль/ко: ч32
	)
_асм ";; ######################## ЗДЕСЬ ЦЕНТР  ФУНКЦИИ ######################## ;;
"

	идти пропустить_первый_вывод

_асм "
	быть еах 1
	быть еди 1
	быть еси текст
	быть едх 20
	сзов
"

	пропустить_первый_вывод:

_асм "
	быть еах 1
	быть еди 1
	быть еси текст
	быть едх 20
	сзов
"

_асм ";; ######################## ЗДЕСЬ КОНЕЦ  ФУНКЦИИ ######################## ;;
"
)

фц глав (аргов/ав: ч32 арги: *стр) тлен (

	вечно (
		вечно (
			идти выход_из_всегда
		)
		вечно (
			вечно ()
			вечно ()
		)
		_асм "
	; здесь то уже тело функции
"		_асм "
	; здесь то тоже тело функции

"
	)
	выход_из_всегда:
)

счет ЧЕТО (
	А
	Б
	В
	Г
	Д 123
	ВЫХОД 77
	Ж -123
	СЧЕТ
)

;

; ЗДЕСЬ МАССИВЫ
пусть масисв_указателей_на_массивы: [**ц64] = [
	&[ &[1 2] &[3 4] ]
	&[ &[5 6] &[7 8] ]
]

пусть массив_на_три_числа: [ч32 5] = [123 123 13]
пусть трехмерный_массив: [[[ц64]]] = [[[1 2] [3 4]] [[5 6 11] [7 8]]]
пусть взятие_указателя_с_массива: *ц64 = &[1488 228 1337]
пусть массив_функций: *(ч32 ч32 ! (тлен)) = &[]

; ЗДЕСЬ ЛИКИ
;;
пусть чел/черт: &лик Чел = {"Владимир" "Владимирович" "Зеленскi"}
пусть черты: [&лик Чел] = [{"Черт" "Черт" "Черт"}]
;;

; ЗДЕСЬ ПРО УКАЗАТЕЛИ
пусть ничего: тлен = 0
пусть ф/фф/ффф: (ч32 ч64 ! ч32) = &ничего
пусть ничего_ук: *ч64 = &ничего
пусть ничего_ч: ч64 = 0
пусть ничего_ч_ук: *тлен = &ничего_ч

; ЗДЕСЬ ПРО ЧИСЛА
;пусть а/б: ч32 = 2 * (123 + 123)
;пусть ук_а: *ч32 = &а
пусть йцук: ц64 = - - -+- 1234
пусть маленькое_число: ц8 = 1234.123
пусть не_число2: в64 = 123
пусть счет_в_банке: ч64 = -ЧЕТО.СЧЕТ

; ЗДЕСЬ ПРО СТРОКИ
пусть строка: стр = &"строка эээ"
пусть строка_строкой: стр = строка

пусть тоже_строка: [ц8 152] = окак [ц8 152] "тоже \"э\"ээ"

пусть не_совсем_строка: *тлен = &"а тут вот не строка или как"
пусть ук_не_совсем_строка: стр = не_совсем_строка
пусть а_вот_не_совсем_даже_строка: *стр = &не_совсем_строка

пусть стр_стр: [стр] = [&"строка1 " &"строка2"]
пусть не_стр_стр: *[стр 2] = &стр_стр

; ОКАК
; пусть совсем_не_строка: *ч32 = окак *ч32 "123"

_асм "
вход здесь
здесь:

вот а 77
вот б а
вот в б
	быть еах 1
	быть еди 1
	быть еси текст
	быть едх 20
	сзов

	зов глав__{ч32_ук_{ч32_ч32_ук_лик_Йцук_счет_ЧЕТО_ч32}_тлен}
	зов глав__{ч32_ук_ук_ц8_тлен}

	быть еах 60
;	быть еди ЧЕТО.ВЫХОД
	быть еди в
	сзов

пусть текст байт \"\т\т\т\т\т\тэээ world!\" 10

"

фц четото (а: ч32, б: ц32, в: в32) ()
фц четото (а: ч32  б: ц32) ()
