;####################################################################################
; ЗДЕСЬ ПРО УКАЗАТЕЛИ
;####################################################################################

пусть ничего: тлен = 0
пусть ф/фф/ффф: (ч32 ч64 ! ч32) = &ничего
пусть ничего_ук: *ч64 = &ничего
пусть ничего_ч: ч64 = 0
пусть ничего_ч_ук: *тлен = &ничего_ч
пусть массива_тлена: *ч64 = &[1 2 3]
;пусть лик_тлена: лик Человек = {"Ну вот короче да"}


;####################################################################################
; ЗДЕСЬ ПРО ЧИСЛА
