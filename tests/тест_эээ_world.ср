
лик Чел (
	имя фамилия отчество прочее/еще: стр
	возраст/взр: ц8

	чето_там: *тлен, ктото_там: [лик Чел], третье: лик Негра
	первое второе тре3ье: ц64

	а: ч32, б: ц32, в/г: в32, ж: ч32
)

фц глав (аргов: ч32 арги: *(ч32 ч32 лик Йцук счет ЧЕТО ! ч32)) тлен (
	пусть (
		а б в г: ч32
		чето_переменная: ц8
		функция: (ч32 ч32 ! ч32)
		не/ско/ль/ко: ч32
	)
)

фц глав (аргов/ав: ч32 арги: *стр) тлен (
	вечно (
		вечно (
			идти выход_из_всегда
		)
		вечно (
			вечно ()
			вечно ()
		)
	)
	выход_из_всегда:
)

счет ЧЕТО (
	А,
	Б,
	В,
	Г,
	Д 123,
	ВЫХОД 77,
	Ж -123,
	СЧЕТ,
)

;####################################################################################
; ЗДЕСЬ МАССИВЫ
;####################################################################################

;пусть а_что_тут_вообще_массив: [[[ч32 4]]] = 0
; пусть не_массив_а_значение: [ч32 2] = 1234
; пусть значение_а_не_массив: ч32 = [123 123 123]
; пусть не_стр_стр: *[стр 2] = &стр_стр

пусть число: ч32 = 1234567
пусть ук_на_число: *ч32 = &число
пусть ук_на_ук_на_число: **ч32 = &ук_на_число
пусть масисв_указателей_на_массивы: [**ч32] = [
	&[ &[1 2], &[3 4] ],
	&[ &[5 6], &[7 8] ],
	;ук_на_ук_на_число TODO: да че так много странного всего
]

пусть массив_на_три_числа: [ч32 5] = [123 123 13]
пусть трехмерный_массив: [[[ц64]]] = [[[1 2 -1] [3 4]] [[5 6] [7 8]]]
пусть взятие_указателя_с_массива: *ц64 = &[1488 228 1337]
пусть массив_функций: *(ч32 ч32 ! (тлен)) = &[]


;####################################################################################
; ЗДЕСЬ ЛИКИ
;####################################################################################

счет ПОЛ (
	М 1,
	Ж 0,
)
лик Человек (
	имя
	фамилия
	отчество: стр

	пол: счет ПОЛ
	челы: [лик Человек 3]
)

пусть предизент: &лик Человек = *{
	"Владимир"
	"Владимирович"
	"Путин"
	ПОЛ.М
	[
		{"Гойдов" "Гойда" "Гойдович" ПОЛ.М []}
	]
}
пусть ВВП: лик Человек = {"Владимир" "Владимирович" "Путин" ПОЛ.М}
;;
;;
; здесь надо еще тесты про лики с массивами и массивы с ликами, и массивы с ликами с массивами
