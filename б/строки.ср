фц __АСМ содержит(стр: стр символ: ц8) ч32 ( _асм "
	; рси - строка
	; ди - символ
	быть дх ди ; дл - символ

	__цикл___содержит__{ук_ц8_ц8_ч32}:
		; сравнивать ал
		быть ал байт(рси)
		; если конец строки
		проб ал ал
		ид0 __воздать_0___содержит__{ук_ц8_ц8_ч32}
		; если нет, то сравнить с символом
		срав ал дл
		идр __воздать_1___содержит__{ук_ц8_ц8_ч32}
	; еще раз
	++рси
	идти __цикл___содержит__{ук_ц8_ц8_ч32}

__воздать_1___содержит__{ук_ц8_ц8_ч32}:
	искл еах еах ++еах
	возд
__воздать_0___содержит__{ук_ц8_ц8_ч32}:
	искл еах еах
")

фц __АСМ длиной(стр: стр) ч32 ( _асм "
	быть рди рси
	искл еах еах
	искл рсх рсх
	не рсх
	орф
	повтн0 осмсб
	минс рди рси
	быть еах еди
	--еах
")

фц __АСМ равняется(стр0 стр1: стр) ч32 ( _асм "

